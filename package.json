{
  "name": "conway",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "compile": "electron-webpack",
    "dev": "electron-webpack dev",
    "dist": "yarn compile && electron-builder",
    "dist-win": "yarn compile && electron-builder --win",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "download-phaser-typedef": "download --out typings https://raw.githubusercontent.com/photonstorm/phaser3-docs/master/typescript/phaser.d.ts",
    "lint": "./node_modules/.bin/eslint ./src/",
    "start": "electron-webpack dev",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ava-ts"
  },
  "dependencies": {
    "phaser": "3.15.1",
    "source-map-support": "0.5.9"
  },
  "devDependencies": {
    "ava": "1.0.0-rc.1",
    "ava-ts": "0.25.2",
    "babel-eslint": "10.0.1",
    "download-cli": "1.1.1",
    "electron": "3.0.7",
    "electron-builder": "20.29.0",
    "electron-webpack": "2.3.1",
    "electron-webpack-ts": "3.0.0",
    "eslint": "5.8.0",
    "esm": "3.2.25",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "webpack": "4.23.1",
    "ya-storage": "1.0.3"
  },
  "resolutions": {
    "upath": "^1.0.5"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          [
            "module:ava/stage-4",
            {
              "modules": false
            }
          ]
        ]
      }
    },
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "ts-node/register"
    ]
  }
}
